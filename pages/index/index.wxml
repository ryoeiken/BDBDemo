<view class='box'>

  <view class='banner'>
    <swiper indicator-dots="{{indicatorDots}}" autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}" circular="{{circular}}">
      <block wx:for="{{swiperImgUrls}}" wx:key="index">
        <swiper-item>
          <image src="{{item}}" />
        </swiper-item>
      </block>
    </swiper>
  </view>


  <view class='nav'>

    <navigator wx:for="{{navImgUrls}}" wx:key="index">
      <image src="{{item}}" />
      <!-- <view wx:for="{{navItems}}"> {{item}} </view> -->
      <view>美食</view>
    </navigator>
  </view>

  <view class='links clearfix'>
    <navigator wx:for="{{linksImgUrls}}" wx:key="index">
      <image src="{{item}}" />
    </navigator>
  </view>
</view>